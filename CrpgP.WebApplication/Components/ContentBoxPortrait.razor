
<div class="content-box">
    <h3>Portrait Box</h3>
    <p>
        name: @DisplayName <br>
        imagePath: @ImagePath <br>
        slug: @Slug <br>
    </p>
</div>


@code {
    [Parameter]
    public required string DisplayName { get; set; }
    [Parameter]
    public required string ImagePath { get; set; }
    [Parameter]
    public required string Slug { get; set; }
}